generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  @@map("users")
  name    String? @db.VarChar(255)
  email   String? @db.VarChar(255)
  address String? @db.VarChar(255)

  id		Int     @id @default(autoincrement())
username	string
password	string
fullName	string
address	string
phone		string
accountType	string
avatar		string

}
